@{
    ViewData["Title"] = "You know what this page is about";
}
<h2>@ViewData["Title"]</h2>
<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=5">
<style>
.parallax {
    /* The image used */
    background-image: url("https://i.stack.imgur.com/pm25i.jpg");

    /* Set a specific height */
    min-height: 700px; 

    /* Create the parallax scrolling effect */
    background-attachment: fixed;
    background-position: center;
    background-repeat: repeat;
    background-size: cover;
}
</style>
</head>
<body>
    
<div class="parallax"></div>

<div style="height:2000px;background-color:black;font-size:56px;">
<p>Ash nazg thrakatulûk agh burzum-ishi krimpatul.</p>
<p>One ring to rule them all, one ring to find them, One ring to bring them all and in the darkness bind them.</p>
</div>

</body>
</html>


