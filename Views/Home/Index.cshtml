@{
    ViewData["Title"] = "Home Page";
}
<!DOCTYPE html>
<html>
<head>
<meta name="viewport" content="width=device-width, initial-scale=5">
<style>
.parallax {
    /* The image used */
    background-image: url("https://www.donsmaps.com/images29/middleearthlargelargerstill.jpg");

    /* Set a specific height */
    min-height: 1000px; 

    /* Create the parallax scrolling effect */
    background-attachment: fixed;
    background-position: center;
    background-repeat: repeat;
    background-size: cover;
}
</style>
</head>
<body>
    
<div class="parallax"></div>

<div style="height:100px;background-color:black;font-size:56px;">
Welcome to the Middle Earth Project System!
</div>

</body>

</html>
